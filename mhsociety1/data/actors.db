value":"common"},"custom":""},"rules":[],"slug":"detect-magic","schema":{"version":0.697,"lastMigration":{"datetime":"2022-01-05T15:05:46.520+00:00","version":{"schema":0.692,"foundry":"0.8.9","system":"2.20.2.9271"}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult","primal"],"custom":""},"school":{"value":"divination"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":"30","areaType":"emanation"},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"k0KIzWi63PB7OMst"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":"30-foot emanation"},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"V16GgjiyWYLP6Yx2":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.gpzpAAAJ1Lza2JVl"}}},{"_id":"iOwY9SOW2CcEWLR9","name":"Gale Blast","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/gale-blast.webp","data":{"description":{"value":"<p>Wind flows from your outstretched hands and whirls around you in a 5-foot emanation. Each creature in the area takes bludgeoning damage equal to your spellcasting ability modifier, with a Fortitude save.</p>\n<hr />\n<p><strong>Critical Success</strong> The creature is unaffected.</p>\n<p><strong>Success</strong> The creature takes half damage.</p>\n<p><strong>Failure</strong> The creature takes full damage and is pushed 5 feet away from you.</p>\n<p><strong>Critical Failure</strong> The creature takes double damage and is pushed 10 feet away from you.</p>\n<hr />\n<p><strong>Heightened (+2)</strong> The damage increases by 1d6.</p>"},"source":{"value":"Pathfinder Secrets of Magic"},"traits":{"value":["air","cantrip"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"gale-blast","schema":{"version":0.697,"lastMigration":{"datetime":"2022-01-05T15:05:46.520+00:00","version":{"schema":0.692,"foundry":"0.8.9","system":"2.20.2.9271"}}},"level":{"value":1},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","primal"],"custom":""},"school":{"value":"evocation"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":"5","areaType":"emanation"},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"applyMod":true,"type":{"categories":[],"subtype":"","value":"bludgeoning"},"value":""}},"applyMod":true},"save":{"value":"fortitude","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"k0KIzWi63PB7OMst"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"scaling":{"damage":{"0":"1d6"},"interval":2}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"V16GgjiyWYLP6Yx2":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.dDiOnjcsBFbAvP6t"}}},{"_id":"hzTbMq194itXeXZ0","name":"Acidic Burst","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/acidic-burst.webp","data":{"description":{"value":"<p>You create a shell of acid around yourself that immediately bursts outward, dealing 2d6 acid damage to each creature in the area.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 2d6.</p>"},"source":{"value":"Pathfinder Lost Omens: Gods & Magic"},"traits":{"value":["acid"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"acidic-burst","schema":{"version":0.697,"lastMigration":{"datetime":"2022-01-05T15:05:46.520+00:00","version":{"schema":0.692,"foundry":"0.8.9","system":"2.